<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.knowwhatyoueat.ui.home.ScannerFragment"
        android:label="@string/title_Suchen"
        tools:layout="@layout/fragment_suche" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.knowwhatyoueat.ui.dashboard.DashboardFragment"
        android:label="@string/title_Scannen"
        tools:layout="@layout/fragment_scanner" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.knowwhatyoueat.ui.notifications.NotificationsFragment"
        android:label="@string/title_Profil"
        tools:layout="@layout/fragment_notifications" />
</navigation>